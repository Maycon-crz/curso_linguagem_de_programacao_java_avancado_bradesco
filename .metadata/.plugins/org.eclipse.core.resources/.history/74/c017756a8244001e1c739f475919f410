public class Layouts extends JFrame{
	
	public Layouts() {
		super("Meu Frame");
		
		JButton botao = new JButton("Bot√£o Centralizado");
		
		getContentPane().add(botao);
		
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setSize(300, 300);
		setVisible(true);
	}

	public static void main(String[] args) {
		new Layouts();
	}

}
